{
  "name": "aifm-frontend-baring",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "start:staging": "ng serve --configuration staging --port 4201",
    "start:production": "ng serve --configuration production --port 4201",
    "build": "npm run build:production",
    "build:production": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@ngrx/effects": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "angular-highcharts": "^17.0.1",
    "bootstrap": "^5.3.8",
    "crypto-js": "^4.2.0",
    "highcharts": "^12.4.0",
    "moment": "^2.30.1",
    "ngrx-store-localstorage": "^20.0.0",
    "nodemon": "^3.1.10",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.0",
    "secure-web-storage": "^1.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
